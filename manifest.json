{
	"manifest_version": 2,
		"name": "Tab Count",
		"version": "1.0",
		"applications": {
			"gecko": {
				"id": "tabcount@grepper.net"
			}
		},
		"permissions": ["tabs"],
		"background": {
			"scripts": ["background.js"],
			"persistent": false
		}, 
		"browser_action": {
			"default_title": "Tab Count",
			"default_area": "tabstrip",
			"browser_style": true
		}
}
